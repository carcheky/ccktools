#!/bin/bash

SERVICE="ssh-agent"
if pgrep -x "$SERVICE" *>/dev/null
then
    echo "$SERVICE is running yet"
else
    echo "starting $SERVICE"
    eval `ssh-agent -s` *>/dev/null
    ssh-add -q ~/.ssh/id_rsa_c* *>/dev/null
fi


source ~/ccktools/.ccktools_aliases
source ~/ccktools/.ccktools_functions
cd /c/wsl/sites
mountdesadrupal