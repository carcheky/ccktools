#!/bin/bash

wsl_self_update(){
  CURRENT_DIR="$(pwd)"

  if [[ -d /c/wsl/user/ccktools ]]; then
    cd /c/wsl/user/ccktools
    git pull
    cd $CURRENT_DIR
  fi

  source ~/ccktools/.ccktools
}
wsl_self_update

source ~/ccktools/.ccktools_aliases
source ~/ccktools/.ccktools_functions

goto
mountdesadrupal